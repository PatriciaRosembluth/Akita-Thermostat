<h1>Registrar ubicación</h1>

<%= form_for :location, url: locations_path do |f| %>
  <p>
    <%= f.label :property , "Propiedad" %>
    <%= f.text_field :property %>
  </p>
 
  <p>
    <%= f.label :ubication , "Dirección" %>
    <%= f.text_field :ubication %>
  </p>
  <p>
    <%= f.label :city , "Ciudad" %>
    <%= f.select :city, ['Cochabamba', 'La Paz', 'Santa Cruz'] %>
  </p>
  <br><br>
  <p>
    <%= f.submit "Registrar" %>
  </p>
  <br>
<% end %>

